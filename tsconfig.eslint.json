{
    "extends": "./tsconfig.json",
    "include": [
        "**/*.ts",
        "**/*.mts",
        "**/*.js",
        "**/*.mjs",
        "**/*.svelte"
    ],
    "exclude": [
        "node_modules",
        "src/service-worker.ts",
        "src/service-worker.d.ts",
        "src/service-worker/**/*.ts",
        "src/service-worker/**/*.d.ts",
        "vite.config.ts.*"
    ]
}
